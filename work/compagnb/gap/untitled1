  if (region == "central"){
    // return '#e82e69';
    return '#fcf7e9';
  }
  if (region == "east"){
    // return '#d92b87';
    return '#fedbad';
  }
  if (region == "west"){
    // return '#c828b6';
    return '#fec9bd';
  }
  if (region == "southern"){
    // return '#a826be';
    return '#fd8c8d';
  }
  if (region == "northern"){
    // return '#9341ea';
    return '#f8b8aa';
  }
